{
  "name": "southwind",
  "version": "1.0.0",
  "description": "Example LOB application using Signum Framework and React",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:dll": "webpack --config webpack.config.dll.js",
    "build:webpack": "webpack --config webpack.config.js",
    "build": "npm run clean && npm run build:dll && npm run build:webpack",
    "watch": "npm run clean && npm run build:dll && webpack --config webpack.config.js --watch --progress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/signumsoftware/southwind"
  },
  "keywords": [
    "Southwind",
    "Signum Framework",
    "LINQ",
    "React"
  ],
  "author": "Signum Software",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/signumsoftware/southwind/issues"
  },
  "homepage": "https://github.com/signumsoftware/southwind",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bpmn-js": "^0.22.1",
    "codemirror": "^5.30.0",
    "d3": "^4.11.0",
    "d3-scale-chromatic": "^1.1.1",
    "es6-promise": "^4.1.1",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "lodash-compat": "^3.10.2",
    "moment": "^2.19.1",
    "moment-duration-format": "^1.3.0",
    "numbro": "^1.11.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^15.6.1",
    "react-overlays": "^0.7.3",
    "react-widgets": "^4.0.2",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-rte": "^0.15.0",
    "react-router-dom": "^4.2.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.0.2",
    "typescript": "^2.5.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-notifier": "^1.5.0"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "watch"
    ]
  }
