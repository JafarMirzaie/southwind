{
  "name": "southwind",
  "version": "1.0.0",
  "description": "Example LOB application using Signum Framework and React",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:dll": "webpack --config webpack.config.dll.js",
    "build:webpack": "webpack --config webpack.config.js",
    "build": "npm run clean && npm run build:dll && npm run build:webpack",
    "watch": "npm run clean && npm run build:dll && webpack --config webpack.config.js --watch --progress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/signumsoftware/southwind"
  },
  "keywords": [
    "Southwind",
    "Signum Framework",
    "LINQ",
    "React"
  ],
  "author": "Signum Software",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/signumsoftware/southwind/issues"
  },
  "homepage": "https://github.com/signumsoftware/southwind",
  "dependencies": {
    "bootstrap": "^4.1.1",
    "bpmn-js": "0.27.5",
    "codemirror": "^5.37.0",
    "d3": "^5.1.0",
    "d3-scale-chromatic": "^1.2.0",
    "es6-promise": "^4.2.4",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "lodash-compat": "^3.10.2",
    "moment": "^2.22.1",
    "moment-duration-format": "^2.2.2",
    "numbro": "^2.0.6",
    "popper.js": "^1.14.3",
    "prop-types": "^15.6.1",
    "query-string": "^6.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-overlays": "^0.8.3",
    "react-popper": "^0.10.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-rte": "^0.16.1",
    "react-widgets": "^4.2.6",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.1.1",
    "typescript": "^2.7.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-notifier": "^1.5.1"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "watch"
    ]
  }
}
