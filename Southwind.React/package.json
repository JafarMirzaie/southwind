{
  "name": "southwind",
  "version": "1.0.0",
  "description": "Example LOB application using Signum Framework and React",
  "main": "index.js",
  "scripts": {
    "preinstall:framework": "yarn --cwd ./../Framework/Signum.React --ignore-scripts install",
    "preinstall:extensions": "yarn --cwd ./../Framework/Signum.React.Extensions --ignore-scripts install",
    "preinstall": "yarn run preinstall:framework && yarn run preinstall:extensions",
    "clean": "rimraf ./wwwroot/dist",
    "build-development": "yarn run clean && webpack --config webpack.config.polyfills.js && webpack --config webpack.config.dll.js && webpack --config webpack.config.js",
    "build-production": "yarn run clean && webpack --config webpack.config.polyfills.js && webpack --config webpack.config.dll.js --mode='production' && webpack --config webpack.config.js --mode='production'",
    "watch": "yarn run clean && webpack --config webpack.config.polyfills.js && webpack --config webpack.config.dll.js && webpack --config webpack.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/signumsoftware/southwind"
  },
  "keywords": [
    "Southwind",
    "Signum Framework",
    "LINQ",
    "React"
  ],
  "author": "Signum Software",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/signumsoftware/southwind/issues"
  },
  "homepage": "https://github.com/signumsoftware/southwind",
  "resolutions": {
    "@types/react": "file:../Framework/Signum.React/node_modules/@types/react"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@types/draft-js": "0.11.1",
    "@types/luxon": "1.26.2",
    "@types/prop-types": "15.7.3",
    "@types/react": "file:../Framework/Signum.React/node_modules/@types/react",
    "abortcontroller-polyfill": "1.7.1",
    "bootstrap": "4.6.0",
    "bpmn-js": "7.5.0",
    "codemirror": "5.60.0",
    "core-js": "3.9.1",
    "d3": "6.6.2",
    "d3-scale-chromatic": "2.0.0",
    "diagram-js-minimap": "2.0.4",
    "draft-js": "0.11.7",
    "draftjs-to-html": "0.9.1",
    "history": "4.10.1",
    "html-to-draftjs": "1.5.0",
    "jquery": "3.5.1",
    "lodash-compat": "3.10.2",
    "@azure/msal-browser": "2.14.1",
    "luxon": "1.26.0",
    "popper.js": "1.16.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-bootstrap": "1.5.2",
    "react-dom": "17.0.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-widgets": "5.0.3",
    "whatwg-fetch": "3.6.2"
  },
  "devDependencies": {
    "assets-webpack-plugin": "7.0.0",
    "css-loader": "5.2.0", 
    "file-loader": "6.2.0",
    "raw-loader": "4.0.2",
    "rimraf": "3.0.2",
    "sass": "1.32.8",
    "sass-loader": "11.0.1", 
    "style-loader": "2.0.0",
    "ts-loader": "9.2.5", 
    "typescript": "4.4.2", 
    "url-loader": "4.1.1",
    "webpack": "5.28.0", 
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.6.0", 
    "webpack-notifier": "1.13.0"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "watch"
    ]
  }
}
