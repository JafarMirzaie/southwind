{
  "name": "southwind",
  "version": "1.0.0",
  "description": "Example LOB application using Signum Framework and React",
  "main": "index.js",
  "scripts": {
    "preinstall:framework": "yarn --cwd ./../Framework/Signum.React --ignore-scripts install",
    "preinstall:extensions": "yarn --cwd ./../Extensions/Signum.React.Extensions --ignore-scripts install",
    "preinstall": "yarn run preinstall:framework && yarn run preinstall:extensions",
    "clean": "rimraf ./wwwroot/dist",
    "build-development": "yarn run clean && webpack --config webpack.config.polyfills.js && webpack --config webpack.config.dll.js && webpack --config webpack.config.js",
    "build-production": "yarn run clean && webpack --config webpack.config.polyfills.js && webpack --config webpack.config.dll.js --mode='production' && webpack --config webpack.config.js --mode='production'",
    "watch": "yarn run clean && webpack --config webpack.config.polyfills.js && webpack --config webpack.config.dll.js && webpack --config webpack.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/signumsoftware/southwind"
  },
  "keywords": [
    "Southwind",
    "Signum Framework",
    "LINQ",
    "React"
  ],
  "author": "Signum Software",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/signumsoftware/southwind/issues"
  },
  "homepage": "https://github.com/signumsoftware/southwind",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.30",
    "@fortawesome/free-brands-svg-icons": "5.14.0",
    "@fortawesome/free-regular-svg-icons": "5.14.0",
    "@fortawesome/free-solid-svg-icons": "5.14.0",
    "@fortawesome/react-fontawesome": "0.1.11",
    "@types/draft-js": "0.10.40",
    "@types/prop-types": "15.7.3",
    "@types/react": "file:../Framework/Signum.React/node_modules/@types/react",
    "abortcontroller-polyfill": "1.4.0",
    "core-js": "3.6.5",
    "bootstrap": "4.5.0",
    "bpmn-js": "5.0.4",
    "codemirror": "5.54.0",
    "d3": "5.16.0",
    "d3-scale-chromatic": "1.5.0",
    "diagram-js-minimap": "2.0.3",
    "dom-helpers": "3.4.0",
    "draft-js": "0.11.5",
    "draftjs-to-html": "0.9.1",
    "history": "4.10.1",
    "html-to-draftjs": "1.5.0",
    "jquery": "3.5.1",
    "lodash-compat": "3.10.2",
    "min-dash": "3.5.2",
    "@types/luxon": "1.24.4",
    "luxon": "1.25.0",
    "popper.js": "1.16.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-bootstrap": "1.0.1",
    "react-dom": "16.13.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-widgets": "4.5.0",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "5.1.1",
    "css-loader": "4.2.2",
    "file-loader": "6.0.0",
    "node-sass": "5.0.0",
    "raw-loader": "4.0.1",
    "rimraf": "3.0.2",
    "sass-loader": "10.1.0",
    "style-loader": "1.2.1",
    "ts-loader": "8.0.11",
    "typescript": "4.1.2",
    "url-loader": "4.1.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-notifier": "1.8.0"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "watch"
    ]
  }
}
