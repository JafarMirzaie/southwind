{
  "name": "southwind",
  "version": "1.0.0",
  "description": "Example LOB application using Signum Framework and React",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:dll": "webpack --config webpack.config.dll.js",
    "build:webpack": "webpack --config webpack.config.js",
    "build": "npm run clean && npm run build:dll && npm run build:webpack",
    "watch": "npm run clean && npm run build:dll && webpack --config webpack.config.js --watch --progress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/signumsoftware/southwind"
  },
  "keywords": [
    "Southwind",
    "Signum Framework",
    "LINQ",
    "React"
  ],
  "author": "Signum Software",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/signumsoftware/southwind/issues"
  },
  "homepage": "https://github.com/signumsoftware/southwind",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bpmn-js": "^0.20.5",
    "codemirror": "^5.25.0",
    "colorbrewer": "^1.0.0",
    "d3": "^3.5.17",
    "es6-promise": "^4.1.0",
    "font-awesome": "^4.7.0",
    "history": "^4.6.1",
    "lodash-compat": "^3.10.2",
    "moment": "^2.18.1",
    "moment-duration-format": "^1.3.0",
    "numbro": "^1.10.1",
    "prop-types": "^15.5.8",
    "query-string": "^4.3.3",
    "react": "^15.5.4",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.5.4",
    "react-overlays": "^0.6.12",
    "react-router": "^4.1.1",
    "react-router-bootstrap": "^0.24.1",
    "react-router-dom": "^4.1.1",
    "react-rte": "^0.11.0",
    "react-widgets": "^3.4.6",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "style-loader": "^0.16.1",
    "ts-loader": "^2.0.3",
    "typescript": "^2.2.2",
    "url-loader": "^0.5.8",
    "raw-loader": "^0.5.1",
    "webpack": "^2.4.1",
