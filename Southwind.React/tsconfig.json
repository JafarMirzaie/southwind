{
  "compilerOptions": {
    "target": "es5",
    //"sourceMap": true,
    "module": "commonjs",
    "jsx": "react",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "../Framework/Signum.React/node_modules/@types/*",
        "../Framework/Signum.React/node_modules/*",
        "*"
      ]
    },
    "lib": [
      "es2017",
      "esnext",
      "dom"
    ]
  },
  "exclude": [ "node_modules" ]
}